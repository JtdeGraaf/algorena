# .env.example - copy to .env and fill in values. DO NOT commit your real .env to VCS.

# Server
SERVER_PORT=8080
SPRING_PROFILES_ACTIVE=dev

# Frontend / Backend URLs (used for CORS and OAuth redirect URIs)
FRONTEND_URL=http://localhost:5173
BACKEND_URL=http://localhost:8080

# JWT (HS256) - provide a strong secret
# Recommended: a 32-byte (256-bit) value encoded as hex (64 hex chars) or base64.
APP_JWT_SECRET=
# Token expiration in milliseconds (default 3600000 = 1 hour)
APP_JWT_EXPIRATION_MS=3600000

# Database (Postgres)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=algorena-db
DB_USERNAME=algorena
DB_PASSWORD=algorena

# OAuth2
GOOGLE_OAUTH_CLIENT_ID=
GOOGLE_OAUTH_CLIENT_SECRET=


